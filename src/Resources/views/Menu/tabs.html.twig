{% apply spaceless %}
    <ul class="menu-tabs list-inline {{ menu.class }}" {{ menu.attributes|raw }}>
        {% for item in menu.items %}
            {% set item = item|merge({'anchor_class': 'btn btn-app ' ~ item.anchor_class}) %}
            {% include '@SheGroupMenu/Menu/item.html.twig' %}
        {% endfor %}
    </ul>
{% endapply %}
